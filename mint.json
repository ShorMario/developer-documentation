{
  "$schema": "https://www.jedpatterson.com/spec.json",
  "name": "Whop Developers",
  "api": {
    "baseUrl": "https://api.whop.com"
  },
  "analytics": {
    "ga4": {
      "measurementId": "G-NGD3HKQGSV"
    }
  },
  "logo": {
    "dark": "/logo/white.png",
    "light": "/logo/whop.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#ff6423",
    "light": "#ff6423",
    "dark": "#ff6423",
