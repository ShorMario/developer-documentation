      "pages": [
        "apps/api/products/list",
        "apps/api/products/retrieve",
        "apps/api/products/retrieve-experiences"
      ]
    },
    {
      "group": "Experiences",
      "pages": ["apps/api/experiences/list", "apps/api/experiences/retrieve"]
    },
    {
      "group": "Users",
      "pages": ["apps/api/users/retrieve", "apps/api/users/get-experiences"]
    },
    {
      "group": "Notifications",
      "pages": ["apps/api/notifications/send"]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/whopio",
    "github": "https://github.com/whopio",
